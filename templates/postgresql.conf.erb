# Generated by Puppet
LoadPlugin PostgreSQL

<Plugin postgresql>
<% [@databases].flatten.each do |db| %>
  <Database <%= db %>>
    Host "<%= @host %>"
    Port "<%= @port %>"
    User "<%= @username %>"
    Password "<%= @password %>"
    SSLMode "prefer"
    Query backends
    Query transactions
    Query queries
    Query table_states
    Query disk_io
    Query disk_usage
  </Database>
<% end %>
</Plugin>
