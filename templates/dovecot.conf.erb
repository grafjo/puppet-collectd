  <File "/var/log/dovecot-info.log">
    Instance "dovecot"
    <Match>
      Regex ".*: Login: "
      DSType "CounterInc"
      Type "counter"
      Instance "login"
    </Match>
    <Match>
      Regex ".*: Login: .* TLS"
      DSType "CounterInc"
      Type "counter"
      Instance "tls"
    </Match>
    <Match>
      Regex ".*: Login: .* secured"
      DSType "CounterInc"
      Type "counter"
      Instance "secured"
    </Match>
    <Match>
      Regex "imap-login: Login:"
      DSType "CounterInc"
      Type "counter"
      Instance "imap"
    </Match>
    <Match>
      Regex "pop3-login: Login:"
      DSType "CounterInc"
      Type "counter"
      Instance "pop"
    </Match>
    <Match>
      Regex ".*: Fatal: "
      DSType "CounterInc"
      Type "counter"
      Instance "fatal"
    </Match>
    <Match>
      Regex ".*: Info: copy"
      DSType "CounterInc"
      Type "counter"
      Instance "imap_copy"
    </Match>
    <Match>
      Regex ".*: Info: delete"
      DSType "CounterInc"
      Type "counter"
      Instance "imap_delete"
    </Match>
    <Match>
      Regex ".*: Info: expunge"
      DSType "CounterInc"
      Type "counter"
      Instance "imap_expunge"
    </Match>
    <Match>
      Regex ".*: Info: undelete"
      DSType "CounterInc"
      Type "counter"
      Instance "imap_undelete"
    </Match>
    <Match>
      Regex ".*: Info: mailbox_delete"
      DSType "CounterInc"
      Type "counter"
      Instance "imap_mailbox_delete"
    </Match>
    <Match>
      Regex ".*: Info: mailbox_rename"
      DSType "CounterInc"
      Type "counter"
      Instance "imap_mailbox_rename"
    </Match>
  </File>
