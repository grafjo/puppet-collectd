  # Fragment by Puppet
  ModulePath "<%= @path %>"
  Import "rabbitmq"
  <Module rabbitmq>
    Verbose <%= @verbose %>
    Username "<%= @username %>"
    Password "<%= @password %>"
    Realm "<%= @realm %>"
    Host "<%= @host %>"
    Port "<%= @port %>"
    <Ignore "queue">
      Regex "amq-gen-.*"
      Regex "tmp-.*"
    </Ignore>
  </Module>
