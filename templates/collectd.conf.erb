# Generated by Puppet
<% unless @fqdnlookup %>
Hostname <%= scope.lookupvar('::fqdn') %>
FQDNLookup false
<% else %>
#Hostname localhost
FQDNLookup true
<% end %>
#BaseDir "/var/lib/collectd"
#PluginDir "/usr/lib/collectd"
TypesDB "/usr/share/collectd/types.db"
Interval <%= @interval %>
Timeout <%= @timeout %>
ReadThreads <%= @threads %>
Include "<%= @plugin_conf_dir %>"
